package kodlama.io.Devs.business.requests.programmingLanguageRequest;

public class UpdateProgrammingLanguageRequest {

	private String name;
	private int id;
	
	
	public UpdateProgrammingLanguageRequest(String name, int id) {
		this.name = name;
		this.id = id;
	}


	public UpdateProgrammingLanguageRequest() {
	
	}


	public String getName() {
		return name;
	}


	public void setName(String name) {
		this.name = name;
	}


	public int getId() {
		return id;
	}


	public void setId(int id) {
		this.id = id;
	}
	
	
	
	
	
	
	
	
	
	  
}
